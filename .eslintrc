{
	"parser": "babel-eslint",
	"env":{
		"node":true,
		"es6":true,
		"mocha":true
	},
	"rules": {
		"strict":0,
		"eqeqeq":0,
		"quotes":0,
		"comma-spacing":0,
		"no-extra-semi":0,
		"no-unused-vars":0,
		"no-ternary":1,
		"no-trailing-spaces":0,
		"no-shadow":0,
		"consistent-return":1
	},
	"ecmaFeaturesj": {
		"spread":true,
		"generators":true
	}
}
